{
  "name": "sbhq-mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:dev": "cross-env APP_VARIANT=dev expo start --dev-client --scheme sbhqmobile-dev --clear",
    "start:dev:tunnel": "cross-env APP_VARIANT=dev expo start --dev-client --scheme sbhqmobile-dev --tunnel",
    "start:tunnel": "expo start --tunnel --go",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --runInBand",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,tsx}\" --ignore-pattern \"tests/**\" --ignore-pattern \"jest.setup.ts\"",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix --ignore-pattern \"tests/**\" --ignore-pattern \"jest.setup.ts\"",
    "format": "prettier \"**/*.{ts,tsx}\" --write",
    "format:check": "prettier \"**/*.{ts,tsx}\" --check"
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.3.1",
    "@expo/vector-icons": "^15.0.3",
    "@lottiefiles/dotlottie-react": "^0.13.5",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/native": "^7.1.8",
    "@supabase/supabase-js": "^2.84.0",
    "expo": "~54.0.30",
    "expo-apple-authentication": "~8.0.8",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.18",
    "expo-device": "~8.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.8",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-notifications": "~0.32.15",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "expo-web-browser": "~15.0.10",
    "lottie-react-native": "^7.3.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.12.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.11.0",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.16",
    "prettier": "^3.4.2",
    "react-native-svg-transformer": "^1.5.2",
    "react-test-renderer": "19.1.0",
    "supabase": "^2.63.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.8.0"
  },
  "private": true
}
