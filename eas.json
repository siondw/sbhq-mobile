{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "autoIncrement": true,
      "channel": "development",
      "env": { "APP_VARIANT": "dev" }
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "channel": "preview",
      "env": { "APP_VARIANT": "prod" }
    },
    "appetize": {
      "channel": "preview",
      "env": { "APP_VARIANT": "prod" },
      "ios": { "simulator": true }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": { "APP_VARIANT": "prod" }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascApiKeyId": "FZD5C64263",
        "ascApiKeyIssuerId": "4dfdd2e6-36c1-4426-a9d5-bfc446e8d01a",
        "ascApiKeyPath": "./private.p8",
        "ascAppId": "6757280045"
      }
    }
  }
}
